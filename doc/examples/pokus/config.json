{
    "parameters": {
        "api": {
            "baseUrl": "http://localhost:8888/pokus/"
        },
        "config": {
            "debug": true,
            "outputBucket": "r2b2",
            "jobs": [
                {
                    "endpoint": "stats",
                    "method": "POST",
                    "params": {
                        "from": {
                            "function": "concat",
                            "args": [
                                "",
                                {
                                    "function": "date",
                                    "args": [
                                        "Y-m-d",
                                        {
                                            "function": "strtotime",
                                            "args": [
                                                "-10 day",
                                                {
                                                    "time": "currentStart"
                                                }
                                            ]
                                        }
                                    ]
                                },
                                "T00:00:00.378Z"
                            ]
                        },
                        "date": {
                            "to": {
                                "function": "concat",
                                "args": [
                                    "",
                                    {
                                        "function": "date",
                                        "args": [
                                            "Y-m-d",
                                            {
                                                "function": "strtotime",
                                                "args": [
                                                    "-1 day",
                                                    {
                                                        "time": "currentStart"
                                                    }
                                                ]
                                            }
                                        ]
                                    },
                                    "T00:00:00.378Z"
                                ]
                            }
                        },
                        "dimensions": [
                            "day",
                            "website",
                            "channel",
                            "placement"
                        ]
                    }
                }
            ]
        }
    }
}
